<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - PROYECTO_TIENDA</title>
    <link rel="stylesheet" href="~/css/site.css" />
    <link rel="stylesheet" href="~/css/AppForms.css" />
    <script src="~/js/Utilidades.js"></script>

</head>
<body>
    <header class="header">
        Tienda "EL HERMANO"
    </header>
    <nav class="nav1">
        <ul class="hList">

            <li>
                <div class="menu">
                    <a href="~/Index">
                        <h2 class="menu-title"><img src="~/Image/home-icon-silhouette_w.png" class="icon" />Inicio</h2>
                    </a>
                </div>
            </li>
            <li>
                <div class="menu">
                    <h2 class="menu-title menu-title_6th"><img src="~/Image/almacen_w.png" class="icon" />Inventario</h2>
                    <ul class="menu-dropdown">
                    </ul>
                </div>
            </li>
            <li>
                <div href="#click" class="menu">
                    <h2 class="menu-title menu-title_2nd"><img src="~/Image/bolsa-de-la-compra_w.png" class="icon" />Compra</h2>
                    <ul class="menu-dropdown">
                        <li><a href="~/Catalogos/CompraView">Compra</a></li>
                    </ul>
                </div>
            </li>
            <li>
                <div class="menu">
                    <h2 class="menu-title menu-title_3rd"><img src="~/Image/catalogo_w.png" class="icon" />
