                    <ul class="menu-dropdown">
                        <li><a href="~/Catalogos/CatView">Categoria</a></li>
                        <li><a href="~/Catalogos/CatViewMarca">Marca</a></li>
                        <li><a href="~/Catalogos/CatViewTalla">Talla</a></li>
                        <li><a href="~/Catalogos/CatViewProducto">Producto</a></li>
                        <li><a href="~/Catalogos/CatViewClientes">Clientes</a></li>
                        <li><a href="~/Catalogos/CatViewUsuario">Usuarios</a></li>
                        <li><a href="~/Catalogos/ProveedoresView">Proveedores</a></li>

                    </ul>
                </div>
            </li>
       
            <li>
                <div class="menu">
                        <h2 class="menu-title"><<img src="~/Image/cupon_w.png" class="icon"/>
                        Reporte</h2>
                </div>
            </li>

        </ul>
    <partial name="_LoginPartial" />
    </nav>
    <main class="AppMain">
        @RenderBody()
    </main>
    <script src="~/js/site.js" asp-append-version="true"></script>

    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
